<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/basicFragment">


    <fragment
        android:id="@+id/basicFragment"
        android:name="in.iot.lab.acl.home.Basic"
        android:label="fragment_basic"
        tools:layout="@layout/fragment_basic" >
        <action
            android:id="@+id/action_basicFragment_to_rv_Sub_grp_button"
            app:destination="@id/rv_Sub_grp_button" />
        <action
            android:id="@+id/action_basicFragment_to_rv_Sub_grp_text"
            app:destination="@id/rv_Sub_grp_text" />
        <action
            android:id="@+id/action_basicFragment_to_rv_Sub_grp_widgets"
            app:destination="@id/rv_Sub_grp_widgets" />
        <action
            android:id="@+id/action_basicFragment_to_rv_Sub_grp_Layouts"
            app:destination="@id/rv_Sub_grp_Layouts" />
        <action
            android:id="@+id/action_basicFragment_to_rv_Sub_grp_DerivedComponents"
            app:destination="@id/rv_Sub_grp_DerivedComponents" />
        <action
            android:id="@+id/action_basicFragment_to_rv_Sub_grp_Containers"
            app:destination="@id/rv_Sub_grp_Containers" />
    </fragment>
    <fragment
        android:id="@+id/advancedFragment"
        android:name="in.iot.lab.acl.AdvancedFragment"
        android:label="fragment_advanced"
        tools:layout="@layout/fragment_advanced" />
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="in.iot.lab.acl.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/rv_Sub_grp_text"
        android:name="in.iot.lab.acl.SubHome.Rv_Sub_grp_text"
        android:label="fragment_rv__sub_grp_text"
        tools:layout="@layout/fragment_rv__sub_grp_text" >
        <action
            android:id="@+id/action_rv_Sub_grp_text_to_textView"
            app:destination="@id/textView" />
        <action
            android:id="@+id/action_rv_Sub_grp_text_to_editText"
            app:destination="@id/editText" />
        <action
            android:id="@+id/action_rv_Sub_grp_text_to_toast"
            app:destination="@id/toast" />
    </fragment>
    <fragment
        android:id="@+id/rv_Sub_grp_button"
        android:name="in.iot.lab.acl.SubHome.Rv_Sub_grp_button"
        android:label="fragment_rv__sub_grp_button"
        tools:layout="@layout/fragment_rv__sub_grp_button" >
        <action
            android:id="@+id/action_rv_Sub_grp_button_to_toggelButtonFrag"
            app:destination="@id/toggelButtonFrag" />
        <action
            android:id="@+id/action_rv_Sub_grp_button_to_imageButtonFrag"
            app:destination="@id/imageButtonFrag" />
        <action
            android:id="@+id/action_rv_Sub_grp_button_to_radioButtonFrag"
            app:destination="@id/radioButtonFrag" />
        <action
            android:id="@+id/action_rv_Sub_grp_button_to_switchFrag"
            app:destination="@id/switchFrag" />
        <action
            android:id="@+id/action_rv_Sub_grp_button_to_checkboxFrag"
            app:destination="@id/checkboxFrag" />
        <action
            android:id="@+id/action_rv_Sub_grp_button_to_floatingActionButtonFrag"
            app:destination="@id/floatingActionButtonFrag" />
    </fragment>
    <fragment
        android:id="@+id/rv_Sub_grp_Containers"
        android:name="in.iot.lab.acl.SubHome.Rv_Sub_grp_Containers"
        android:label="fragment_rv__sub_grp__containers"
        tools:layout="@layout/fragment_rv__sub_grp__containers" />
    <fragment
        android:id="@+id/rv_Sub_grp_DerivedComponents"
        android:name="in.iot.lab.acl.SubHome.Rv_Sub_grp_DerivedComponents"
        android:label="fragment_rv__sub_grp__derived_components"
        tools:layout="@layout/fragment_rv__sub_grp__derived_components" />
    <fragment
        android:id="@+id/rv_Sub_grp_Layouts"
        android:name="in.iot.lab.acl.SubHome.Rv_Sub_grp_Layouts"
        android:label="fragment_rv__sub_grp__layouts"
        tools:layout="@layout/fragment_rv__sub_grp__layouts" >
        <action
            android:id="@+id/action_rv_Sub_grp_Layouts_to_constraintLayoutFrag"
            app:destination="@id/constraintLayoutFrag" />
        <action
            android:id="@+id/action_rv_Sub_grp_Layouts_to_linearLayoutH"
            app:destination="@id/linearLayoutH" />
        <action
            android:id="@+id/action_rv_Sub_grp_Layouts_to_linearLayoutV"
            app:destination="@id/linearLayoutV" />
        <action
            android:id="@+id/action_rv_Sub_grp_Layouts_to_frameLayoutFrag"
            app:destination="@id/frameLayoutFrag" />
        <action
            android:id="@+id/action_rv_Sub_grp_Layouts_to_tableLayoutFrag"
            app:destination="@id/tableLayoutFrag" />
    </fragment>
    <fragment
        android:id="@+id/rv_Sub_grp_widgets"
        android:name="in.iot.lab.acl.SubHome.Rv_Sub_grp_widgets"
        android:label="fragment_rv__sub_grp_widgets"
        tools:layout="@layout/fragment_rv__sub_grp_widgets" >
        <action
            android:id="@+id/action_rv_Sub_grp_widgets_to_imageViewFrag"
            app:destination="@id/imageViewFrag" />
    </fragment>
    <fragment
        android:id="@+id/textView"
        android:name="in.iot.lab.acl.text.textView"
        android:label="fragment_text_view"
        tools:layout="@layout/fragment_text_view" />
    <fragment
        android:id="@+id/editText"
        android:name="in.iot.lab.acl.text.EditText"
        android:label="fragment_edit_text"
        tools:layout="@layout/fragment_edit_text" />
    <fragment
        android:id="@+id/toast"
        android:name="in.iot.lab.acl.text.Toast"
        android:label="fragment_toast"
        tools:layout="@layout/fragment_toast" />
    <fragment
        android:id="@+id/checkboxFrag"
        android:name="in.iot.lab.acl.button.CheckboxFrag"
        android:label="fragment_checkbox"
        tools:layout="@layout/fragment_checkbox" />
    <fragment
        android:id="@+id/floatingActionButtonFrag"
        android:name="in.iot.lab.acl.button.FloatingActionButtonFrag"
        android:label="fragment_floating_action_button"
        tools:layout="@layout/fragment_floating_action_button" />
    <fragment
        android:id="@+id/imageButtonFrag"
        android:name="in.iot.lab.acl.button.ImageButtonFrag"
        android:label="fragment_image_button"
        tools:layout="@layout/fragment_image_button" />
    <fragment
        android:id="@+id/radioButtonFrag"
        android:name="in.iot.lab.acl.button.RadioButtonFrag"
        android:label="fragment_radio_button"
        tools:layout="@layout/fragment_radio_button" />
    <fragment
        android:id="@+id/switchFrag"
        android:name="in.iot.lab.acl.button.SwitchFrag"
        android:label="fragment_switch"
        tools:layout="@layout/fragment_switch" />
    <fragment
        android:id="@+id/toggelButtonFrag"
        android:name="in.iot.lab.acl.button.ToggelButtonFrag"
        android:label="fragment_toggel_button"
        tools:layout="@layout/fragment_toggel_button" />
    <fragment
        android:id="@+id/imageViewFrag"
        android:name="in.iot.lab.acl.widgets.ImageViewFrag"
        android:label="fragment_image_view"
        tools:layout="@layout/fragment_image_view" />
    <fragment
        android:id="@+id/constraintLayoutFrag"
        android:name="in.iot.lab.acl.layouts.ConstraintLayoutFrag"
        android:label="fragment_constraint_layout"
        tools:layout="@layout/fragment_constraint_layout" />
    <fragment
        android:id="@+id/frameLayoutFrag"
        android:name="in.iot.lab.acl.layouts.FrameLayoutFrag"
        android:label="fragment_frame_layout"
        tools:layout="@layout/fragment_frame_layout" />
    <fragment
        android:id="@+id/linearLayoutH"
        android:name="in.iot.lab.acl.layouts.LinearLayoutH"
        android:label="fragment_linear_layout_h"
        tools:layout="@layout/fragment_linear_layout_h" />
    <fragment
        android:id="@+id/linearLayoutV"
        android:name="in.iot.lab.acl.layouts.LinearLayoutV"
        android:label="fragment_linear_layout_v"
        tools:layout="@layout/fragment_linear_layout_v" />
    <fragment
        android:id="@+id/tableLayoutFrag"
        android:name="in.iot.lab.acl.layouts.TableLayoutFrag"
        android:label="fragment_table_layout"
        tools:layout="@layout/fragment_table_layout" />
</navigation>